<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="ee3f970b-35d2-4a2d-99c5-e677ce5e8544" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/9.0/project-config.xsd">
  <name>feature branches</name>
  <description />
  <settings>
    <options>
      <option name="allowExternalStatus" value="true" />
      <option name="checkoutMode" value="ON_AGENT" />
      <option name="cleanBuild" value="true" />
    </options>
    <parameters>
      <param name="branch.default" value="refs/heads/develop" />
      <param name="branch.specification"><![CDATA[+:refs/heads/*
-:refs/heads/master]]></param>
      <param name="chocolatey.api.key" value="zxxe10d1f514444181d8220131010da6206bf932b2d40d24b09e00c530493f5025af894f7d1f490d06a" spec="password display='normal'" />
      <param name="chocolatey.api.url" value="http://localhost:8080/" />
      <param name="crane.buildnumber" value="0" />
      <param name="vcs.checkout.rules" value="+:/" />
      <param name="vcs.trigger.rules"><![CDATA[-:user=cranebuild
+:/src
+:/build]]></param>
    </parameters>
    <build-runners>
      <runner id="RUNNER_1" name="Build" type="jetbrains_powershell">
        <parameters>
          <param name="jetbrains_powershell_bitness" value="x64" />
          <param name="jetbrains_powershell_execution" value="PS1" />
          <param name="jetbrains_powershell_script_code"><![CDATA[.\build.ps1 -tasklist @("TeamCityBuildStep") -build_number %build.number% -teamcity_build -chocolatey_api_key %chocolatey.api.key% -chocolatey_api_url %chocolatey.api.url%


Write-Host LastExitCode: $LASTEXITCODE
Write-Host Error Count: $($Error.Count)
<#
if ($LASTEXITCODE -ne 0 -and $LASTEXITCODE -ne $null)
{
   Write-Host "Error in build exiting with $LASTEXITCODE"
   exit $LASTEXITCODE
}

if ($Error.Count -ne 0)
{
   Write-Host $Error[0].Exception
   Write-Host "Error in build exiting with -1"
   exit -1
}
#>]]></param>
          <param name="jetbrains_powershell_script_file" value="build.ps1" />
          <param name="jetbrains_powershell_script_mode" value="CODE" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_3" name="Generate documentation" type="jetbrains_powershell">
        <parameters>
          <param name="jetbrains_powershell_bitness" value="x64" />
          <param name="jetbrains_powershell_execution" value="PS1" />
          <param name="jetbrains_powershell_script_code"><![CDATA[.\build.ps1 -tasklist @("generatedocs")

Write-Host git add doc/*
git add doc/*

Write-Host git add mkdocs.yml
git add mkdocs.yml

Write-Host git commit -m "Updated documentation build no: %build.number%"
git commit -m "Updated documentation build no: %build.number%"

Write-Host git push origin %teamcity.build.branch%
git push origin %teamcity.build.branch%

<#
if ($LASTEXITCODE -ne 0 -and $LASTEXITCODE -ne $null)
{
   Write-Host "Error in build exiting with $LASTEXITCODE"
   exit $LASTEXITCODE
}

if ($Error.Count -ne 0)
{
   Write-Host $Error[0].Exception
   Write-Host "Error in build exiting with -1"
   exit -1
}
#>]]></param>
          <param name="jetbrains_powershell_script_mode" value="CODE" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_8" name="Tweet new build" type="jetbrains_powershell">
        <parameters>
          <param name="jetbrains_powershell_bitness" value="x64" />
          <param name="jetbrains_powershell_execution" value="PS1" />
          <param name="jetbrains_powershell_script_code"><![CDATA[Import-Module .\build\modules\twitter.psm1 -Force
Send-Tweet -Message "New bleeding edge %build.number% of crane from the %teamcity.build.branch% branch. cinst crane -source http://chocolatey.cranebuild.com:8080/nuget"]]></param>
          <param name="jetbrains_powershell_script_mode" value="CODE" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="crane_CraneGit">
        <checkout-rule rule="%vcs.checkout.rules%" />
      </vcs-entry-ref>
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="vcsTrigger" type="vcsTrigger">
        <parameters>
          <param name="perCheckinTriggering" value="true" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
          <param name="triggerRules" value="%vcs.trigger.rules%" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <cleanup />
  </settings>
</build-type>

